<UserControl x:Class="VehicleInspectionInterface.EditCar"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             Height="200" Width="200">
    <StackPanel>
        <TextBox x:Name="MarkTextBox" Margin="5"/>
        <TextBox x:Name="ModelTextBox" Margin="5"/>
        <TextBox x:Name="YearTextBox" Margin="5"/>
        <TextBox x:Name="NumberTextBox" Margin="5"/>
        <CheckBox x:Name="InspectionCheckBox" Content="Техосмотр пройден" Margin="5"/>
        <Button Content="Сохранить" Click="SaveButton_Click" Margin="5"/>
    </StackPanel>
</UserControl>
